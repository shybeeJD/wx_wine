.roundedRectangle {
  overflow: auto;
  border-radius: 5px;
  box-shadow: 1px 1px 5px #cdcfcf, 1px 1px 5px #cdcfcf;
  background-color: #fff;
}
.flexContainerCol {
  display: flex;
  flex-direction: column;
}
.flexContainerRow {
  display: flex;
  flex-direction: row;
}
.align_centerContainer {
  display: flex;
  align-items: center;
}
.justify_centerContainer {
  display: flex;
  justify-content: center;
}
.allCenterContainer {
  display: flex;
  align-items: center;
  justify-content: center;
}
.justifyContentSpaceBetween {
  display: flex;
  justify-content: space-between;
}
.justifyContentLeft {
  display: flex;
  justify-content: flex-start;
}
.justifyContentRight {
  display: flex;
  justify-content: flex-end;
}
.padding_l_10px {
  padding: 0px 0px 0px 10px;
}
.padding_r_10px {
  padding: 0px 10px 0px 0px;
}
.padding_l_r_10px {
  padding: 0px 10px 0px 10px;
}
/*捎话wx.css  */
.section {
  width: 96%;
  height: 220rpx;
  margin-left: 5px;
}
.message1 {
  width: 96%;
  height: 210rpx;
  margin-top: 30rpx;
  font-size: 90%;
  margin-left: 20px;
}
.circle {
  height: 35rpx;
  width: 35rpx;
  border-radius: 50%;
  display: inline-block;
  margin: 28rpx 45rpx auto 50rpx;
}
.fast {
  position: relative;
  top: -50rpx;
  font-size: 90%;
  background: #f4f4f4;
  line-height: 100rpx;
  text-indent: 2em;
}
.item {
  position: relative;
  border-bottom: 1px solid #f4f4f4;
  padding-bottom: 10px;
  top: -60rpx;
}
.message_font {
  font-size: 80%;
  font-family: "微软雅黑";
  font-weight: blod;
  display: inline-block;
  position: relative;
  top: -5rpx;
}
.submit {
  width: 100%;
  height: 100rpx;
  background: #fed1d6;
  position: relative;
  top: 168rpx;
  line-height: 100rpx;
}
button::after {
  border: none;
}
