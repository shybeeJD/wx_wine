.roundedRectangle {
  overflow: auto;
  border-radius: 5px;
  box-shadow: 1px 1px 5px #cdcfcf, 1px 1px 5px #cdcfcf;
  background-color: #ffffff;
}
.flexContainerCol {
  display: flex;
  flex-direction: column;
}
.flexContainerRow {
  display: flex;
  flex-direction: row;
}
.align_centerContainer {
  display: flex;
  align-items: center;
}
.justify_centerContainer {
  display: flex;
  justify-content: center;
}
.allCenterContainer {
  display: flex;
  align-items: center;
  justify-content: center;
}
.justifyContentSpaceBetween {
  display: flex;
  justify-content: space-between;
}
.justifyContentLeft {
  display: flex;
  justify-content: flex-start;
}
.justifyContentRight {
  display: flex;
  justify-content: flex-end;
}
.padding_l_10px {
  padding: 0px 10px 0px 10px;
}
.padding_l_r_10px {
  padding: 0px 10px 0px 10px;
}
.shop_ui {
  height: 56px;
  font-size: medium;
  margin: 10px 0 10px 0;
  flex-direction: column;
  display: flex;
  justify-content: center;
  padding: 0px 10px 0px 10px;
}
.shop_ui .shopTitle {
  display: flex;
  align-items: center;
  flex: 4;
}
.shop_ui .shopTitle .block {
  padding: 0px 10px 0px 10px;
}
.shop_ui .shopInfo {
  flex: 2;
  display: flex;
  align-items: center;
  font-size: smaller;
  color: gray;
}
.shop_ui .moreInfo {
  flex: 1;
  display: flex;
  align-items: center;
  font-size: smaller;
  color: gray;
}
.shop_ui .moreInfo .text {
  width: 100%;
  text-align: right;
}
page {
  height: 100%;
}
.content {
  width: 100%;
  height: 100%;
}
/*背景滚动试图*/
.shop_ui {
  z-index: 10;
}
.scroll-content {
  background-color: #f5f5f5;
}
/*轮播图*/
.scroll-image {
  width: 100%;
}
/*轮播图 item*/
.scroll-image-item {
  width: 100%;
  height: 100%;
  ime-mode: aspectFit;
}
/*icon 背景图*/
.icon-back-style {
  width: 100%;
  height: 200px;
}
/*一个icon背景*/
.one_icon_back_view {
  width: 25%;
  height: 100%;
  float: left;
}
/*icon样式*/
.icon_size {
  width: 60px;
  height: 60px;
  display: block;
  margin-left: auto;
  margin-right: auto;
  margin-top: 8px;
}
/*icon的标题*/
.icon_title {
  font-size: 14px;
  text-align: center;
  margin-top: 8px;
  display: block;
  width: 100%;
}
.icon_back_size {
  width: 100%;
  height: 50%;
}
/*热卖商品背景*/
.host_good_back {
  width: 100%;
  background-color: white;
  position: relative;
}
/*热卖商品左侧右侧背景*/
.host_good_left_right_back {
  float: left;
  position: relative;
  overflow: hidden;
}
/*热卖商品主图*/
.host_good_main_image {
  text-align: center;
}
/*商品标题*/
.host_good_title {
  display: block;
  margin-right: auto;
  margin-left: auto;
  width: 90%;
  height: 38px;
  font-size: 16px;
  line-height: 19px;
  text-align: center;
  overflow: hidden;
  /* 内容超出宽度时隐藏超出部分的内容 */
  text-overflow: ellipsis;
}
/*商品tags*/
.host_good_tags {
  background-color: red;
  font-size: 10px;
  color: white;
  border-radius: 3px;
  margin-left: 3px;
  line-height: 10px;
  float: left;
}
/*商品tags背景*/
.host_tags_back {
  width: 90%;
  height: 20px;
  display: block;
  margin-right: auto;
  margin-left: auto;
  padding-top: 5px;
}
/*价钱*/
.host_price {
  font-size: 16px;
  color: red;
  margin-top: 10px;
  margin-left: 5%;
}
/*商品市场价*/
.host_mark_price {
  font-size: 13px;
  color: #333333;
  margin-top: 10px;
  margin-left: 3px;
  text-decoration: line-through;
}
/*加号*/
.add_image_style {
  width: 30px;
  height: 30px;
  position: relative;
  top: -10px;
  right: 10px;
  float: right;
}
/*减号*/
.reduce_image_style {
  width: 30px;
  height: 30px;
  position: relative;
  top: -10px;
  right: 10px;
  float: right;
}
/*购买商品数量*/
.buy_good_number {
  height: 20px;
  position: relative;
  top: -3px;
  right: 10px;
  float: right;
  font-size: 16px;
  margin-right: 3px;
  margin-left: 3px;
}
.underDevelopmentContainer {
  width: 100%;
  height: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.underDevelopmentContainer .underDevelopment {
  font-size: 24px;
}
