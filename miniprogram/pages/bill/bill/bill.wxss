.roundedRectangle {
  overflow: auto;
  border-radius: 5px;
  box-shadow: 1px 1px 5px #cdcfcf, 1px 1px 5px #cdcfcf;
  background-color: #fff;
}
.flexContainerCol {
  display: flex;
  flex-direction: column;
}
.flexContainerRow {
  display: flex;
  flex-direction: row;
}
.align_centerContainer {
  display: flex;
  align-items: center;
}
.justify_centerContainer {
  display: flex;
  justify-content: center;
}
.allCenterContainer {
  display: flex;
  align-items: center;
  justify-content: center;
}
.justifyContentSpaceBetween {
  display: flex;
  justify-content: space-between;
}
.justifyContentLeft {
  display: flex;
  justify-content: flex-start;
}
.justifyContentRight {
  display: flex;
  justify-content: flex-end;
}
.padding_l_10px {
  padding: 0px 0px 0px 10px;
}
.padding_r_10px {
  padding: 0px 10px 0px 0px;
}
.padding_l_r_10px {
  padding: 0px 10px 0px 10px;
}
.c {
  background-color: #fff;
}
.c .item {
  font-size: x-small;
  border-radius: 30px;
  padding: 0 10px;
  margin: 5px 10px;
  border: gray solid 1px;
}
.c .itemSelected {
  font-size: x-small;
  border-radius: 30px;
  padding: 0 10px;
  margin: 5px 10px;
  border: #ff585a solid 1px;
  color: #ff585a;
}
.c .billType {
  flex-direction: row;
  display: flex;
  align-items: center;
}
.c .type {
  margin: 10px 10px;
}
.c .riseType {
  flex-direction: row;
  display: flex;
  align-items: center;
}
.c .riseName .class1 {
  flex-direction: row;
  display: flex;
  align-items: center;
}
.c .riseName .class1 .content {
  display: flex;
  align-items: center;
  margin: 10px 10px;
  font-size: x-small;
}
.c .riseName .class2 {
  display: flex;
  flex-direction: column;
}
.c .riseName .class2 .class2_item {
  flex-direction: row;
  display: flex;
  align-items: center;
}
.c .riseName .class2 .class2_item .content {
  flex-direction: row;
  display: flex;
  align-items: center;
  margin: 10px 10px;
  font-size: x-small;
}
.c .btn {
  position: fixed;
  bottom: 0;
  margin-bottom: 20px;
  width: 100%;
  height: 32px;
  border-radius: 30px;
  color: #fff;
  background-color: #ff585a;
  display: flex;
  align-items: center;
  justify-content: center;
}
