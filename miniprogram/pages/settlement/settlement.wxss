.c1 {
  display: flex;
  flex-direction: column;
  min-height: 150px;
  margin: 10px;
  overflow: auto;
  border-radius: 5px;
  box-shadow: 1px 1px 5px #cdcfcf, 1px 1px 5px #cdcfcf;
}
.underLine {
  width: 100%;
  height: 1px;
  display: inline;
  padding-top: 10upx;
  margin: 0 auto;
  border-bottom: 1px solid #a3a2a2;
}
.c2 {
  display: flex;
  flex-direction: column;
  min-height: 224px;
  margin: 10px;
  overflow: auto;
  border-radius: 5px;
  box-shadow: 1px 1px 5px #cdcfcf, 1px 1px 5px #cdcfcf;
}
.c2 .c2_title {
  background-color: #F1F1F1;
  flex: 1;
  padding: 0px 10px 0px 10px;
  display: flex;
  align-items: center;
  font-size: 12pt;
  font-weight: bold;
  min-height: 84rpx;
}
.c2 .c2_goodsList {
  background-color: #E2E2E2;
  display: flex;
  align-items: center;
  flex: 1;
  font-size: 12pt;
}
.c2 .c2_goodsList .list {
  display: flex;
  flex-direction: column;
  min-height: 84rpx;
  padding: 0 10px 0 10px;
  margin-bottom: 10px;
  width: 100%;
}
.c2 .c2_goodsList .list .item {
  display: flex;
  align-items: center;
  flex: 1;
  min-height: 84rpx;
}
.c2 .c2_goodsList .list .item .title {
  flex: 2;
}
.c2 .c2_goodsList .list .item .num {
  flex: 1;
  text-align: right;
}
.c2 .c2_goodsList .list .item .price {
  flex: 1;
  text-align: right;
}
.c2 .c2_discount {
  background-color: #F1F1F1;
  display: flex;
  align-items: center;
  flex: 1;
  padding: 0px 10px 0px 10px;
  min-height: 84rpx;
}
.c2 .c2_total {
  background-color: #E2E2E2;
  display: flex;
  height: 84rpx;
  align-items: center;
  flex: 1;
  padding: 0px 10px 0px 10px;
  min-height: 84rpx;
  margin-left: auto;
}
.notes {
  background: #E2E2E2;
  min-height: 84rpx;
  margin: 10px;
  display: flex;
  align-items: center;
  padding: 0px 10px 0px 10px;
  overflow: auto;
  border-radius: 5px;
  box-shadow: 1px 1px 5px #cdcfcf, 1px 1px 5px #cdcfcf;
}
.Submit {
  width: 100%;
  background: #d1d1d1;
  min-height: 84rpx;
  position: absolute;
  bottom: 0;
  display: flex;
  align-items: center;
}
